# Cron files
files:
    "/etc/cron.d/moodle-cron":
        mode: "000644"
        owner: root
        group: root
        content: |
            * * * * *  webapp . /opt/elasticbeanstalk/support/envvars-wrapper.sh /usr/bin/php /var/www/html/admin/cli/cron.php > /var/tmp/lastcron.txt

commands:
    remove_old_cron:
        command: "rm -f /etc/cron.d/*.bak"
